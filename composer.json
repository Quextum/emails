  "keywords": [
    "nette"
  ],
  "type": "library",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "require": {
    "php": ">=8.0",
    "nette/utils": "^3.0",
    "tracy/tracy": "^2.6",
    "nette/http": "^3.0",
    "nette/caching": "^3.0",
    "nette/schema": "^1.0",
    "nette/di": "^3.0",
    "ext-json": "*",
    "latte/latte": "^2.10",
    "nette/application": "^3.1",
    "nette/mail": "^3.1"
  },
  "require-dev": {
    "nette/tester": "^2.0"
  },
  "autoload": {
    "psr-4": {
