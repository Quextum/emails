{layout @layout.latte}
{block content}
<article>
    <table>
        <tbody>
        <tr>
            <th>{_'email'}:</th>
            <td>{$email}</td>
        </tr>
        <tr>
            <th>{_'name'}:</th>
            <td>{$name}</td>
        </tr>
        <tr>
            <th>{_'message'}:</th>
            <td>{$message|breaklines|noescape}</td>
        </tr>
        </tbody>
    </table>
</article>
